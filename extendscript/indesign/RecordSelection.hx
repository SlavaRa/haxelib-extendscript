package extendscript.indesign;

/*
 * The records to merge.
 */
typedef RecordSelection = {
  /*
   * Merges all records.
   */
  var ALL_RECORDS:Float;
  /*
   * Merges the specified record.
   */
  var ONE_RECORD:Float;
  /*
   * Merges all records in the specified range.
   */
  var RANGE:Float;
};