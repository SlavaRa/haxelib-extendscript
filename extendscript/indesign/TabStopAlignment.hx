package extendscript.indesign;

/*
 * Tab stop alignment options.
 */
typedef TabStopAlignment = {
  /*
   * Left.
   */
  var LEFT_ALIGN:Float;
  /*
   * Center.
   */
  var CENTER_ALIGN:Float;
  /*
   * Right.
   */
  var RIGHT_ALIGN:Float;
  /*
   * Aligns the specified character with the tab stop.
   */
  var CHARACTER_ALIGN:Float;
};