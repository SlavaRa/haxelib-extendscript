package extendscript.indesign;

/*
 * Character set options exporting tagged text files.
 */
typedef TagTextExportCharacterSet = {
  /*
   * ASCII.
   */
  var ASCII:Float;
  /*
   * ANSI.
   */
  var ANSI:Float;
  /*
   * Unicode.
   */
  var UNICODE:Float;
  /*
   * Shift_JIS.
   */
  var SHIFT_JIS:Float;
  /*
   * Uses GB18030 encoding.
   */
  var GB18030:Float;
  /*
   * Uses KSC5601 encoding.
   */
  var KSC5601:Float;
  /*
   * Chinese Big 5
   */
  var CHINESE_BIG_5:Float;
  /*
   * The Central European (ISO) character set.
   */
  var CENTRALEUROPEAN_ISO:Float;
  /*
   * The Cyrillic (ISO) character set.
   */
  var CYRILLIC_ISO:Float;
  /*
   * The Greek (ISO) character set.
   */
  var GREEK_ISO:Float;
  /*
   * The Windows Arabic character set.
   */
  var WINDOWS_ARABIC:Float;
  /*
   * The Windows Hebrew character set.
   */
  var WINDOWS_HEBREW:Float;
};