package jsx.indesign;

/*
 * OpenType positional form options.
 */
typedef PositionalForms = {
  /*
   * None.
   */
  var NONE:Float;
  /*
   * Calculated forms.
   */
  var CALCULATE:Float;
  /*
   * Initial form.
   */
  var INITIAL:Float;
  /*
   * Medial form.
   */
  var MEDIAL:Float;
  /*
   * Final form.
   */
  var FINAL:Float;
  /*
   * Isolated form.
   */
  var ISOLATED:Float;
};