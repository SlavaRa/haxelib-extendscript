package extendscript.indesign;

/*
 * Style options for kenten characters.
 */
typedef KentenCharacter = {
  /*
   * Does not use kenten.
   */
  var NONE:Float;
  /*
   * Uses the kenten sesame dot.
   */
  var KENTEN_SESAME_DOT:Float;
  /*
   * Uses the kenten white sesame dot.
   */
  var KENTEN_WHITE_SESAME_DOT:Float;
  /*
   * Uses the kenten black circle.
   */
  var KENTEN_BLACK_CIRCLE:Float;
  /*
   * Uses the kenten white circle.
   */
  var KENTEN_WHITE_CIRCLE:Float;
  /*
   * Uses the kenten black triangle.
   */
  var KENTEN_BLACK_TRIANGLE:Float;
  /*
   * Uses the kenten white triangle.
   */
  var KENTEN_WHITE_TRIANGLE:Float;
  /*
   * Uses the kenten bullseye.
   */
  var KENTEN_BULLSEYE:Float;
  /*
   * Uses the kenten fisheye.
   */
  var KENTEN_FISHEYE:Float;
  /*
   * Uses the kenten small black circle.
   */
  var KENTEN_SMALL_BLACK_CIRCLE:Float;
  /*
   * Uses the kenten small white circle.
   */
  var KENTEN_SMALL_WHITE_CIRCLE:Float;
  /*
   * Uses a custom kenten style.
   */
  var CUSTOM:Float;
};