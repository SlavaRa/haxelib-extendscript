package extendscript.indesign;

/*
 * Number variable numbering styles.
 */
typedef VariableNumberingStyles = {
  /*
   * Current
   */
  var CURRENT:Float;
  /*
   * Arabic numerals.
   */
  var ARABIC:Float;
  /*
   * Uppercase Roman numerals.
   */
  var UPPER_ROMAN:Float;
  /*
   * Lowercase Roman numerals.
   */
  var LOWER_ROMAN:Float;
  /*
   * Uppercase letters.
   */
  var UPPER_LETTERS:Float;
  /*
   * Lowercase letters.
   */
  var LOWER_LETTERS:Float;
  /*
   * Single leading zero.
   */
  var SINGLE_LEADING_ZEROS:Float;
  /*
   * Double leading zeros.
   */
  var DOUBLE_LEADING_ZEROS:Float;
  /*
   * Kanji.
   */
  var KANJI:Float;
  /*
   * Full-width Arabic numerals.
   */
  var FULL_WIDTH_ARABIC:Float;
};