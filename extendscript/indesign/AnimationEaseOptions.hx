package jsx.indesign;

/*
 * The ease option.
 */
typedef AnimationEaseOptions = {
  /*
   * no easing.
   */
  var NO_EASE:Float;
  /*
   * simple ease in.
   */
  var EASE_IN:Float;
  /*
   * simple ease out.
   */
  var EASE_OUT:Float;
  /*
   * simple ease in and out.
   */
  var EASE_IN_OUT:Float;
  /*
   * cutom read only ease type.
   */
  var CUSTOM_EASE:Float;
};