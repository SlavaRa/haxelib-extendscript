package extendscript.indesign;

/*
 * PDF export page layout options.
 */
typedef PageLayoutOptions = {
  /*
   * Uses default page layout.
   */
  var DEFAULT_VALUE:Float;
  /*
   * Single page layout.
   */
  var SINGLE_PAGE:Float;
  /*
   * Single page continuous layout.
   */
  var SINGLE_PAGE_CONTINUOUS:Float;
  /*
   * Two up facing page layout.
   */
  var TWO_UP_FACING:Float;
  /*
   * Two up facing continuous page layout.
   */
  var TWO_UP_FACING_CONTINUOUS:Float;
  /*
   * Two up cover page page layout.
   */
  var TWO_UP_COVER_PAGE:Float;
  /*
   * Two up cover page continuous page layout.
   */
  var TWO_UP_COVER_PAGE_CONTINUOUS:Float;
};