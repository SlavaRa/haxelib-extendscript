package extendscript.indesign;

/*
 * Text orientation options.
 */
typedef StoryHorizontalOrVertical = {
  /*
   * Orients the text horizontally.
   */
  var HORIZONTAL:Float;
  /*
   * Orients the text vertically.
   */
  var VERTICAL:Float;
  /*
   * The text direction is unknown.
   */
  var UNKNOWN:Float;
};