package extendscript.indesign;

/*
 * Playback options.
 */
typedef AnimationPlayOperations = {
  /*
   * Starts playback.
   */
  var PLAY:Float;
  /*
   * Stops playback.
   */
  var STOP:Float;
  /*
   * Pauses playback.
   */
  var PAUSE:Float;
  /*
   * Resumes playback.
   */
  var RESUME:Float;
  /*
   * Reverses playback.
   */
  var REVERSE_PLAYBACK:Float;
  /*
   * Stops all playback.
   */
  var STOP_ALL:Float;
};