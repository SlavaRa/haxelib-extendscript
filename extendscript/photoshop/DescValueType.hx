package extendscript.photoshop;

/*
 * The value type of an object.
 */
typedef DescValueType = {
  /*
   * Integer.
   */
  var INTEGERTYPE:Int;
  /*
   * Double.
   */
  var DOUBLETYPE:Int;
  /*
   * Unit value of type double.
   */
  var UNITDOUBLE:Int;
  /*
   * String.
   */
  var STRINGTYPE:Int;
  /*
   * Boolean.
   */
  var BOOLEANTYPE:Int;
  /*
   * Action list.
   */
  var LISTTYPE:Int;
  /*
   * Object.
   */
  var OBJECTTYPE:Int;
  /*
   * Enumeration.
   */
  var ENUMERATEDTYPE:Int;
  /*
   * Reference.
   */
  var REFERENCETYPE:Int;
  /*
   * Class.
   */
  var CLASSTYPE:Int;
  /*
   * Alias.
   */
  var ALIASTYPE:Int;
  /*
   * Raw.
   */
  var RAWTYPE:Int;
};