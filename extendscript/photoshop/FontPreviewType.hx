package extendscript.photoshop;

/*
 * The type size to use for font previews in the Type tool font menus.
 */
typedef FontPreviewType = {
  /*
   * No preview.
   */
  var NONE:Int;
  /*
   * Small type.
   */
  var SMALL:Int;
  /*
   * Medium type.
   */
  var MEDIUM:Int;
  /*
   * Large type.
   */
  var LARGE:Int;
};