package extendscript.photoshop;

/*
 * Color models.
 */
typedef ColorModel = {
  /*
   * Grayscale.
   */
  var GRAYSCALE:Int;
  /*
   * RGB.
   */
  var RGB:Int;
  /*
   * CMYK.
   */
  var CMYK:Int;
  /*
   * Lab.
   */
  var LAB:Int;
  /*
   * HSB.
   */
  var HSB:Int;
  /*
   * The color model has not yet been assigned.
   */
  var NONE:Int;
};