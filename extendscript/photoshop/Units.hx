package extendscript.photoshop;

/*
 * The measurement unit for ruler increments.
 */
typedef Units = {
  /*
   * Pixels.
   */
  var PIXELS:Int;
  /*
   * Inches.
   */
  var INCHES:Int;
  /*
   * Centimeters.
   */
  var CM:Int;
  /*
   * Millimeters.
   */
  var MM:Int;
  /*
   * Points.
   */
  var POINTS:Int;
  /*
   * Picas.
   */
  var PICAS:Int;
  /*
   * Percent.
   */
  var PERCENT:Int;
};