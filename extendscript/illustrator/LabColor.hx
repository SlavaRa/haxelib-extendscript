package extendscript.illustrator;

/*
 * An Lab color specification.
 */
typedef LabColor = {
  /*
   * The L color value (between 0.0 and 100.0)
   */
  var l:Float;
  /*
   * The a color value (between -128.0 and 127.0)
   */
  var a:Float;
  /*
   * The b color value (between -128.0 and 127.0)
   */
  var b:Float;
};