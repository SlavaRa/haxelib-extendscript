package extendscript.illustrator;

/*
 * The transparency grid for the new document, in video presets.
 */
typedef DocumentTransparencyGrid = {
  /*
   * No grids.
   */
  var TransparencyGridNone:Int;
  /*
   * Light grid colors.
   */
  var TransparencyGridLight:Int;
  /*
   * Medium grid colors.
   */
  var TransparencyGridMedium:Int;
  /*
   * Dark color grids.
   */
  var TransparencyGridDark:Int;
  /*
   * Red grids.
   */
  var TransparencyGridRed:Int;
  /*
   * Orange grids.
   */
  var TransparencyGridOrange:Int;
  /*
   * Green grids.
   */
  var TransparencyGridGreen:Int;
  /*
   * Blue grids.
   */
  var TransparencyGridBlue:Int;
  /*
   * Purple grids.
   */
  var TransparencyGridPurple:Int;
};