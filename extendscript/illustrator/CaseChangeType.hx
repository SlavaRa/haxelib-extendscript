package extendscript.illustrator;

/*
 * The case change type.
 */
typedef CaseChangeType = {
  /*
   * Change to upper case.
   */
  var UPPERCASE:Int;
  /*
   * Change to lower case.
   */
  var LOWERCASE:Int;
  /*
   * Change to title case.
   */
  var TITLECASE:Int;
  /*
   * Change to sentence case.
   */
  var SENTENCECASE:Int;
};