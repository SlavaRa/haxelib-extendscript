package jsx.photoshop;

/*
 * The formatting for the filename extension.
 */
typedef Extension = {
  /*
   * Does not use an extension.
   */
  var NONE:Int;
  /*
   * The extension is in lowercase letters.
   */
  var LOWERCASE:Int;
  /*
   * The extension is in uppercase letters.
   */
  var UPPERCASE:Int;
};