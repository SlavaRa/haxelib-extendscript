package jsx.indesign;

/*
 * Playback options.
 */
typedef PlayOperations = {
  /*
   * Starts playback.
   */
  var PLAY:Float;
  /*
   * Stops playback.
   */
  var STOP:Float;
  /*
   * Pauses playback.
   */
  var PAUSE:Float;
  /*
   * Resumes playback.
   */
  var RESUME:Float;
  /*
   * Stops all playback (SWF only).
   */
  var STOP_ALL:Float;
};