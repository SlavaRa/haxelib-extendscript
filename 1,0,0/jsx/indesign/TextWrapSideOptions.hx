package jsx.indesign;

/*
 * Text wrap side options.
 */
typedef TextWrapSideOptions = {
  /*
   * Both sides text wrap.
   */
  var BOTH_SIDES:Float;
  /*
   * Left side text wrap.
   */
  var LEFT_SIDE:Float;
  /*
   * Right side text wrap.
   */
  var RIGHT_SIDE:Float;
  /*
   * Binding side text wrap.
   */
  var SIDE_TOWARDS_SPINE:Float;
  /*
   * Away from binding side text wrap.
   */
  var SIDE_AWAY_FROM_SPINE:Float;
  /*
   * Largest side text wrap.
   */
  var LARGEST_AREA:Float;
};