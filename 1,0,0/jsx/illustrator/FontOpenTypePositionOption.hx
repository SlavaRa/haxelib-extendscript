package jsx.illustrator;

/*
 * The OpenType font position option.
 */
typedef FontOpenTypePositionOption = {
  /*
   * Default position.
   */
  var OPENTYPEDEFAULT:Int;
  /*
   * OpenType superscript position.
   */
  var OPENTYPESUPERSCRIPT:Int;
  /*
   * OpenType subscript position.
   */
  var OPENTYPESUBSCRIPT:Int;
  /*
   * OpenType numerator position.
   */
  var NUMERATOR:Int;
  /*
   * OpenType denominator position.
   */
  var DENOMINATOR:Int;
};